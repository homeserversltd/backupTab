{
    "backup_items": [
      "/tmp/test.txt"
    ],
    "providers": {
      "aws_s3": {
        "enabled": false,
        "credentials_key": "aws_s3",
        "container": "homeserver-backups",
        "container_type": "bucket",
        "username": "",
        "password": ""
      },
      "google_drive": {
        "enabled": false,
        "credentials_key": "google_drive",
        "container": "HOMESERVER Backups",
        "container_type": "folder",
        "username": "",
        "password": ""
      },
      "dropbox": {
        "enabled": false,
        "credentials_key": "dropbox",
        "container": "/HOMESERVER Backups",
        "container_type": "folder",
        "username": "",
        "password": ""
      },
      "backblaze": {
        "enabled": false,
        "credentials_key": "backblaze",
        "container": "homeserver-backups",
        "container_type": "bucket",
        "username": "",
        "password": ""
      }
    },
    "retention_days": 30,
    "encryption_enabled": true,
    "logging": {
      "enabled": true,
      "log_file": "/var/log/homeserver/backup.log",
      "log_level": "INFO",
      "max_file_size_mb": 10,
      "backup_count": 5,
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  }
  