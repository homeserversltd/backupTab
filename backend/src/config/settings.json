{
    "backup_items": [
      "/tmp/test.txt"
    ],
    "compression": {
      "enabled": true,
      "level": 6
    },
    "encryption": {
      "enabled": true
    },
    "timestamp_chains": {
      "enabled": true,
      "format": "%Y%m%d_%H%M%S"
    },
    "providers": {
      "local": {
        "enabled": true,
        "container": "/mnt/nas/backups/homeserver"
      },
      "google_cloud_storage": {
        "enabled": false,
        "credentials_key": "google_cloud_storage",
        "container": "homeserver-backups",
        "container_type": "bucket",
        "credentials_file": "gcs_credentials.json",
        "bucket_name": "homeserver-backups",
        "project_id": "",
        "max_retries": 3,
        "retry_delay": 1.0,
        "timeout": 300,
        "username": "",
        "password": ""
      },
      "aws_s3": {
        "enabled": false,
        "credentials_key": "aws_s3",
        "container": "homeserver-backups",
        "container_type": "bucket",
        "bucket_name": "homeserver-backups",
        "region": "us-west-2",
        "keyman_integrated": true,
        "keyman_service_name": "aws_s3",
        "max_retries": 3,
        "retry_delay": 1.0,
        "timeout": 300
      },
      "backblaze": {
        "enabled": false,
        "credentials_key": "backblaze",
        "container": "homeserver-backups",
        "container_type": "bucket",
        "keyman_integrated": true,
        "keyman_service_name": "backblaze",
        "region": "us-west-000",
        "max_retries": 3,
        "retry_delay": 1.0,
        "timeout": 300,
        "max_bandwidth": null,
        "upload_chunk_size": 104857600,
        "encryption_enabled": false,
        "encryption_key": null,
        "encryption_salt": null,
        "connection_pool_size": 5
      }
    },
    "retention_days": 30,
    "encryption_enabled": true,
    "backup_count": 0,
    "logging": {
      "enabled": true,
      "log_file": "/var/log/homeserver/backup.log",
      "log_level": "INFO",
      "max_file_size_mb": 10,
      "backup_count": 5,
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  }
  